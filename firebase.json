{
  "hosting": {
    "public": "dist/video-player",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
    "emulators": {
  
      "hosting": {
        "port": 5000,
        "host": "0.0.0.0"
      }
    }
  
}
